#include <stdio.h>
#include <sys/stat.h>
#include <unistd.h>
#include <fcntl.h>

int main() {

  struct stat test;

  stat("testfile.txt",test);

  printf("Showing stats of <testfile.txt>");
  pritnf("Size: %d",test.st_size);
  pritnf("Permissions: %d",test.st_mode);
  pritnf("Access time: %d",test.st_atime);
  

  return 0;
}
